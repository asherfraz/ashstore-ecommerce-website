{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/app.ts"
        }
    ]
}